{
  "extends": "@react-native/typescript-config/tsconfig.json",
 "compilerOptions": {
    "target": "es6",
    "module": "es2015",
    "jsx": "react-native",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*", "App.tsx"],
  "exclude": ["node_modules"]
}
